{
  "manifest_version": 2,
  "name": "nojs",
  "version": "0.0.1",
  "permissions": [
    "tabs",
    "contentSettings",
    "contextMenus",
    "storage"
  ],
  // "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_title": "Toggle JavaScript"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
